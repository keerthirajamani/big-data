package dd;
import java.util.Scanner;

public class angle {
public static void main(String[]args){
	float a,b,c;
	Scanner scan = new Scanner(System.in);
	System.out.println("enter hour");
	a=scan.nextInt();
	System.out.println("enter minute");
	b=scan.nextInt();
	c=(b*11/2)-(a*30);
	
	System.out.println("Angle is  "+c+"  !!!");
scan.close();
}
}
